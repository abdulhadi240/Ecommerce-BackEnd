{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "subtitile": {
      "type": "string"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "size": {
      "type": "json"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Cost_Price": {
      "type": "decimal"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "Featured": {
      "type": "string"
    },
    "MinStock": {
      "type": "integer"
    },
    "sub_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::sub-category.sub-category",
      "inversedBy": "products"
    },
    "mas_unit_of_measurment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::mas-unit-of-measurment.mas-unit-of-measurment",
      "inversedBy": "products"
    },
    "MaxStock": {
      "type": "integer"
    },
    "Remarks": {
      "type": "text"
    }
  }
}
